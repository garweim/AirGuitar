
  <div class="container-landing">
    <div class="  container-index-booking">
      <h3> My bookings </h3>
      <div class="container-large-card">
        <% @bookings.each do |booking| %>
          <div class="large-card">
            <div class="img-large-card">
              <%= cl_image_tag booking.offering.picture, alt: "user picture" %>
            </div>
            <div class="card-trip-content">
              <div class="card-trip-description">
                <h4>You booked <%= booking.offering.name %></h4>
                <p>From <%= booking.starting_date %></p>
                <p>Until <%= booking.end_date %></p>
              </div>
            </div>
          </div>
        <!-- if current_user == booking.user -->
        <% end %>
      </div>
    </div>
</div>



